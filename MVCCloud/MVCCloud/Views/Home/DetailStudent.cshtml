@model MVCCloud.Models.Student

@{
    ViewData["Title"] = " Student Profile";
}
<h4>EMployee Detail information</h4>
<h1>@Model.name</h1>

<div style="border: solid 1px rgba(100, 100, 100, .20); box-shadow: 0 3px 8px rgba(0, 0, 0, .20);">
    
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.id)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.id)
        </dd>

        <dt class="col-sm-2">
          Born year
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.year)
        </dd>
        <dt class="col-sm-2">
          Current address
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.address)
        </dd>
        

    </dl>
</div>
<div>
    <a> 
        @Html.ActionLink("Edit ", "GetEditStudent", "Home", new { id = Model.id })
    </a>|
    <a asp-action="Index" >Go to home</a>
</div>
